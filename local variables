class Test{
    public static void main(String[] args) {
        Student s1=new Student();
      
       s1.show();
}// Us
}
class Student {
   int a=10;
   static int b=20;
   void show(){
       int c=30;
       System.out.println("a="+a);
       System.out.println("b="+b);
       System.out.println("c="+c);
   }
}
